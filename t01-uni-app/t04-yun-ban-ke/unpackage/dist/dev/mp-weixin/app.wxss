:root,
page {
	/* 状态栏高度 */
	--statusBarHeight: 20px;
	/* 导航栏高度 */
	--navBarHeight: 44px;
	/* 行高 */
	--lineHeight: 1.8;
	/* 内外边距 */
	--gutterS: 10rpx;
	--gutterM: 20rpx;
